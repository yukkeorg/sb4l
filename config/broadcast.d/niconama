VBITRATE="260"
FPS="30"

ASAMPLINGRATE="44100"
ABITRATE="96"
ACHANNEL="2"
VCODEC_OPTS="-vcodec libx264 -preset fast 
             -partitions i4x4,p8x8,p4x4,b8x8,i8x8 
             -flags +loop 
             -crf 26
             -maxrate ${VBITRATE}k -bufsize $((${VBITRATE} * 3 / 2))k
             -qmin 10 -qmax 51 
             -keyint_min 0"

ACODEC_OPTS="-acodec libfaac"
OUTPUT_OPTS="-f flv -s 512x384"
